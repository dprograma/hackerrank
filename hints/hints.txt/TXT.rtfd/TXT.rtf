def pangrams(s):\
    # Convert the string to lowercase to ignore case\
    s = s.lower()\
    \
    # Create a set of all alphabets in the English language\
    alphabets = set("abcdefghijklmnopqrstuvwxyz")\
    \
    # Iterate through each character in the string\
